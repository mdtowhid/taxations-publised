@using Taxations.Models;
@{
    UserInformation userInformation = new UserInformation();
    userInformation = ViewBag.UserInformations;
    BasicInformation bi = new BasicInformation();

    if (ViewBag.BasicInformations != null)
    {
        bi = ViewBag.BasicInformations;
    }
    TotalInfo totalInfo = new TotalInfo();
    if (ViewBag.AllInformations != null)
    {
        totalInfo = ViewBag.AllInformations;
    }
    FinalCalculation fc = new FinalCalculation();
    if (ViewBag.FinalCalculations != null)
    {
        fc = ViewBag.FinalCalculations;
    }

    ParticularsOfIncomeFromSalary poifs = new ParticularsOfIncomeFromSalary();
    if (ViewBag.ParticularsOfSalaries != null)
    {
        poifs = ViewBag.ParticularsOfSalaries;
    }

    ParticularsOfTaxCredit ptc = new ParticularsOfTaxCredit();
    if (ViewBag.TaxCredits != null)
    {
        ptc = ViewBag.TaxCredits;
    }

    StatementOfAsset soa = new StatementOfAsset();
    if (ViewBag.StatementOfAssets != null)
    {
        soa = ViewBag.StatementOfAssets;
    }

    Surcharge surcharge = new Surcharge();

    if (ViewBag.Surcharges != null)
    {
        surcharge = ViewBag.Surcharges;
    }
}



<div id="basicInformationPartTwoId" class="form-slides">
    <input type="hidden" value="@soa.NetWealth16" id="totalAssets"/>
    <div class="row border m-1">
        <div class="col-md-12 text-center p-3">
            <h3>Particulars of Income and Tax</h3>
        </div>
    </div>

    <div class="row border m-1 bg-light">
        <div class="col-md-8 p-2 border-right">
            <h3 class="m-0">Particulars of Total Income</h3>
        </div>
        <div class="col-md-4 p-2 ">
            <h3 class="m-0 text-center">Amount</h3>
        </div>
    </div>

    <div class="row border m-1 mt-minus">
        <div class="col-md-1 border-right pt-3 text-center">24</div>
        <div class="col-md-7 border-right pt-3">Salaries (annex Schedule 24A)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.21</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" readonly id="Salaries" name="Salaries" value="@poifs.Total"
                            class="form-control 24-33" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus">
        <div class="col-md-1 border-right pt-3 text-center">25</div>
        <div class="col-md-7 border-right pt-3">Interest on securities</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.22</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="InterestOnSecurities" value="@bi.InterestOnSecurities"
                            name="InterestOnSecurities" class="form-control 24-33" readonly />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">26</div>
        <div class="col-md-7 border-right pt-3">Income from house property (annex Schedule 24B)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.24</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="IncomeFromHouseProperty" name="IncomeFromHouseProperty"
                            class="form-control 24-33" readonly />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">27</div>
        <div class="col-md-7 border-right pt-3">Agricultural income</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.26</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input readonly type="text" id="AgriculturalIncome" name="AgriculturalIncome"
                            class="form-control 24-33" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">28</div>
        <div class="col-md-7 border-right pt-3">Income from business or profession (annex Schedule 24C)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.28</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="IncomeFromBusinessOrProfession" name="IncomeFromBusinessOrProfession"
                            class="form-control 24-33" readonly />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">29</div>
        <div class="col-md-7 border-right pt-3">Capital gains</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.31</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="CapitalGains" name="CapitalGains" class="form-control 24-33" readonly />
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">30</div>
        <div class="col-md-7 border-right pt-3">Income from other sources</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.33</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="IncomeFromOtherSources" name="IncomeFromOtherSources"
                            class="form-control 24-33" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">31</div>
        <div class="col-md-7 border-right pt-3">Share of income from firm or AOP</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="ShareOfIncomeFromFirmOrAOP" name="ShareOfIncomeFromFirmOrAOP"
                            class="form-control 24-33" />
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">32</div>
        <div class="col-md-7 border-right pt-3">Income of minor or spouse under section 43(4)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3 border-right">S.43</div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="IncomeOfMinor" name="IncomeOfMinor" class="form-control 24-33" />
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">33</div>
        <div class="col-md-7 border-right pt-3">Foreign income</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="ForeignIncome" name="ForeignIncome" class="form-control 24-33" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">34</div>
        <div class="col-md-7 border-right pt-3">Total income (aggregate of 24 to 33)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" readonly id="TotalIncome" name="TotalIncome" class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!--::::::::::::::::::::::::::::::::::::::::-->

    <div class="row border m-1 bg-light mt-3">
        <div class="col-md-8 p-2 border-right">
            <h3 class="m-0">Tax Computation and Payment</h3>
        </div>
        <div class="col-md-4 p-2 ">
            <h3 class="m-0 text-center">Amount</h3>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">35</div>
        <div class="col-md-7 border-right pt-3">Gross tax before tax rebate</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" readonly id="GrossTaxBeforeTaxRebate" name="GrossTaxBeforeTaxRebate"
                            value="@poifs.GrossTaxBeforeTaxRebate" class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">36</div>
        <div class="col-md-7 border-right pt-3">Tax rebate (annex Schedule 24D)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="TaxRebate" name="TaxRebate" value="@ptc.AmountOfTaxRebateCalculated"
                            readonly class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">37</div>
        <div class="col-md-7 border-right pt-3">Net tax after tax rebate</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="NetTaxAfterTaxRebate" name="NetTaxAfterTaxRebate" value="" readonly
                            class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">38</div>
        <div class="col-md-7 border-right pt-3">Minimum tax</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="MinimumTax" name="MinimumTax" value="5000" readonly
                            class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">39</div>
        <div class="col-md-7 border-right pt-3">Net wealth surcharge</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="NetWealthSurcharge" name="NetWealthSurcharge"
                            value="@bi.NetWealthSurcharge" readonly class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">40</div>
        <div class="col-md-7 border-right pt-1">Interest or any other amount under the Ordinance (if any)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="InterestOrOrdinance" name="InterestOrOrdinance" class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">41</div>
        <div class="col-md-7 border-right pt-3">Total amount payable</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="TotalAmountPayable" name="TotalAmountPayable"
                            value="@bi.TotalAmountPayable" readonly class="form-control addFor43" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">42</div>
        <div class="col-md-7 border-right pt-3">Tax deducted or collected at source (attach proof)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="number" id="TaxDeducted" name="TaxDeducted" value="@bi.TaxDeducted"
                            class="form-control total414243 46" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">43</div>
        <div class="col-md-7 border-right pt-3">Advance tax paid (attach proof)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="number" id="AdvanceTaxPaid" name="AdvanceTaxPaid" value="@bi.AdvanceTaxPaid"
                            class="form-control 46total total414243  46" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">44</div>
        <div class="col-md-7 border-right pt-1">Adjustment of tax refund [mention assessment year(s) of refund]</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="AdjustmentOfTaxRefund" name="AdjustmentOfTaxRefund"
                            class="form-control 46total total414243 46" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">45</div>
        <div class="col-md-7 border-right pt-3">Amount paid with return (attach proof)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="AmountPaidWithReturn" name="AmountPaidWithReturn"
                            value="@bi.AmountPaidWithReturn" readonly class="form-control 46total 46" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">46</div>
        <div class="col-md-7 border-right pt-3">Total amount paid and adjusted (42+43+44+45)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="TotalAmountPaidAndAdjusted" name="TotalAmountPaidAndAdjusted" readonly
                            class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">47</div>
        <div class="col-md-7 border-right pt-3">Deficit or excess (refundable) (41-46)</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="number" id="DeficitOrExcess" name="DeficitOrExcess" class="form-control"
                            readonly />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row border m-1 mt-minus border-top-0">
        <div class="col-md-1 border-right pt-3 text-center">48</div>
        <div class="col-md-7 border-right pt-3">Tax exempted income</div>
        <div class="col-md-4">
            <div class="container m-0 p-0">
                <div class="row">
                    <div class="col-md-3 pt-3"></div>
                    <div class="col-md-9 pb-2 pt-2">
                        <input type="text" id="TaxExemptedIncome" readonly name="TaxExemptedIncome"
                            value="@poifs.TotalTaxExempted" class="form-control" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
