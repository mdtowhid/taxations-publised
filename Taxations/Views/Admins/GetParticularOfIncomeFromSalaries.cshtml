                    {
                        <img src="@Url.Content(item.PhotoPath)" />
                    }

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployeementStatus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalTaxExempted)
                </td>
                <td>
