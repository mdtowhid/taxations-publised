                    @Html.DisplayFor(modelItem => item.GrossTaxBeforeTaxRebateMaxPercentage)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalSurcharge)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SurchargePercentage)
                </td>
                <td>
                    @Html.ActionLink("Details", "GetParticularOfIncomeFromSalaries", new { id = item.Id }, new { @class = "border btn btn-lg btn-secondary bg-dark text-white" })
                </td>
            </tr>
